<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.javastart.fxmedia.MainController">
   <children>
      <VBox alignment="BOTTOM_CENTER">
         <children>
            <MediaView id="mediaView" fx:id="mediaView" />
            <Button id="pausePlayButton" fx:id="pausePlayButton" mnemonicParsing="false" text="Pause/Play" />
         </children>
      </VBox>
   </children>
</AnchorPane>
